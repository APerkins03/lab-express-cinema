
<h1>Movies Page</h1>

{{#each Movies}}
<div class="movie-container">
  <div class="image-container">
    <img src="{{this.image}}" alt="">
  </div>
  <div class="movie-details">
 
   <h2>{{this.title}}</h2> <a href="/movies/{{this._id}}">See More</a>
   <a href="/movies/{{this._id}}/movie-edit">Edit</a> 
    
    
  </div>
</div>
{{/each}}